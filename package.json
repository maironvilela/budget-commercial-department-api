  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --poll --inspect  --transpile-only --ignore-watch node_modules --respawn src/main/server.ts",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "yarn test -- --watch ",
    "test:staged": "yarn test --",
    "test:ci": "yarn test -- --coverage",
    "prepare": "husky install",
    "build": "rimraf dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typeorm:run": "yarn typeorm migration:run",
    "typeorm:revert": "yarn typeorm migration:revert"
  },
